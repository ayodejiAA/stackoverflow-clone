{
  "name": "stackoverflow-clone",
  "version": "1.0.0",
  "description": "A simple clone of Stackoverflow",
  "scripts": {
    "test": "jest --detectOpenHandles --verbose --collect-coverage --forceExit",
    "lint": "eslint ./src/**/*.ts --quiet --fix",
    "start": "tsc && node ./dist/app.js",
    "dev": "cross-env DEBUG=app:* nodemon ./src/index.ts",
    "db:create:db": "sequelize db:create",
    "db:create:db-test": "cross-env NODE_ENV=test sequelize db:create",
    "db:migrate": "sequelize db:migrate",
    "db:migrate-undo": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:rollback": "npm run db:migrate-undo && npm run db:migrate && npm run db:seed",
    "db:rollback-test": "cross-env NODE_ENV=test npm run db:rollback"
  },
  "author": "Ayodeji Afolabi",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "mysql2": "^2.2.5",
    "sequelize": "^6.4.0",
    "slugify": "^1.4.6",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.22",
    "@types/sequelize": "^4.28.9",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/yamljs": "^0.2.31",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "yamljs": "^0.3.0"
  }
}